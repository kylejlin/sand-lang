pub class Try {
    statement() {
        let a = b;

        try {
            c();
        } catch (e: Exception) {
            d();
        }
        
        d
    }

    noCatchBinding() {
        let a = b;

        try {
            c();
        } catch {
            d();
        }
        
        d
    }

    multipleCatches() {
        try {
            a();
        } catch (e: FooException) {

        } catch (e: BarException) {
            
        } catch {

        }
    }

    lastStatement() {
        try {
            a
        } catch {
            b
        }
    }
}