pub class Try {
    statement() {
        let a = b;

        try {
            c();
        } catch (e: Exception) {
            d();
        }
        
        d
    }

    restrictedBindingless() {
        try {
            a();
        } catch instanceof Foo {
            b();
        }
    }

    union() {
        try {
            a();
        } catch instanceof Foo | Bar | Baz {
            b();
        }
    }

    catchAll() {
        let a = b;

        try {
            c();
        } catch {
            d();
        }
        
        d
    }

    multipleCatches() {
        try {
            a();
        } catch (e: FooException) {

        } catch (e: BarException) {
            
        } catch instanceof Foo | Bar | Baz {
            
        } catch {

        }
    }
}