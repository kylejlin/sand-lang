pub class TypedObjectLiterals {
    empty() {
        Foo {}
    }

    withEntries() {
        Foo {
            baz: 8,
            bar: 9,
            qux: "yo"
        }
    }

    trailingComma() {
        Foo {
            baz: 8,
            bar: 9,
            qux: "yo",
        }
    }

    typeArgs() {
        Foo<int, String> {
            baz: 8,
            bar: 9,
            qux: "yo",
        }
    }

    shorthand() {
        Foo {
            baz
        }
    }

    shorthandTrailingComma() {
        Foo {
            baz,
        }
    }

    shorthandTypeArgs() {
        Foo<int> {
            baz
        }
    }

    mixed() {
        Foo<int> {
            baz: 8,
            bar,
            qux: "yo",
        }
    }
}