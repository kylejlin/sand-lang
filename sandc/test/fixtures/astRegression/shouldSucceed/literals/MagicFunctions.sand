pub class MagicFunctions {
    simple() {
        \n -> n * n
    }

    ifReturn() {
        \n -> if n < 0 {
            -n
        } else {
            n
        }
    }

    curried() {
        \x -> \y -> x + y
    }

    inForIteratee() {
        for n in (1..=10).map(\n -> n * n) {
            System.out.println(n);
        }
    }
}