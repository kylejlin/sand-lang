pub class IfMinus {
    foo() {
        -if x {}
    }

    foo2() {
        -if x {} x
    }

    foo3() {
        -if x {} - x
    }

    foo4() {
        -if x {} if x {} x
    }

    foo5() {
        -if x {} if x {} x
    }

    foo6() {
        -if x {} - if x {}
    }

    foo7() {
        -if x {} - if x {}; x
    }

